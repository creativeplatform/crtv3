# Next.js Environment Variables
# Replace these values with your own before running the application

# Public Variables (accessible in browser)
NEXT_PUBLIC_TEMPLATE_CLIENT_ID=your_template_client_id
NEXT_PUBLIC_ALCHEMY_API_KEY=your_alchemy_api_key
NEXT_PUBLIC_WALLET_CONNECT_PROJECT_ID=your_wallet_connect_project_id
NEXT_PUBLIC_THIRDWEB_CLIENT_ID=your_thirdweb_client_id
NEXT_PUBLIC_THIRDWEB_SECRET_KEY=your_thirdweb_secret_key
NEXT_PUBLIC_LIVEPEER_API_KEY=your_livepeer_api_key

# Private Variables (server-side only)
LIVEPEER_API_KEY=your_livepeer_api_key
JWT_SECRET=your_jwt_secret_at_least_32_chars_long

# Development Settings
NODE_ENV=development

# API URLs
NEXT_PUBLIC_API_URL=http://localhost:3000/api
NEXT_PUBLIC_APP_URL=http://localhost:3000

# New environment variables
DATABASE_URL="postgresql://user:password@localhost:5432/dbname"
THIRDWEB_SECRET_KEY="YOUR_THIRDWEB_SECRET_KEY"
THIRDWEB_AUTH_DOMAIN="localhost:3000"

# SUPPORT URL
NEXT_PUBLIC_SUPPORT_URL="Support URL" 